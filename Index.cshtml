<h1>Here are some delicious foods!</h1>
@{
    List<string> foods = new List<string>
        {
            "Apple Pie",
            "Pizza",
            "Cinnamon Rolls",
            "Lasagna",
            "Donuts",
            "Chocolate Cake",
            "Burritos"
        };
}
<ul>
    @foreach (var food in foods)
    {
        <li>@food</li>
    }
</ul>